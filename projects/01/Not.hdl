/**
 * Not gate:
 * if (in == 0) out = 1, else out = 0
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    //Nand(a=in, b=0, out=outa)
    //Nand(a=0, b=in, out=outb)
    Nand(a=in, b=in, out=out);
}
